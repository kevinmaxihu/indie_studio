## CNA_groundhog_2018
## File description:
## Makefile
##

default_target: all

all:
	@cp Makefile tmp/
	-@./.start.sh
	@cp tmp/Makefile ./

clean:
	@cp Makefile tmp/
	-@./.start.sh clean
	@cp tmp/Makefile ./

fclean:
	@cp Makefile tmp/
	-@./.start.sh fclean
	@cp tmp/Makefile ./

re:
	@cp Makefile tmp/
	-@./.start.sh re
	@cp tmp/Makefile ./

.PHONY: all compile clean fclean re
